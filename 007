#include<string>
#include<iostream>
using namespace std;

int main()
{
    string arr[3];
    for(int i=0;i<3;i++)
        cin>>arr[i];
    int n=3;
    while(n)
    {
        for(int i=0;i<2;i++)
        {
            if(arr[i]>arr[i+1])
            {
                swap(arr[i],arr[i+1]);
            }
            if(arr[i].length()>arr[i+1].length())
                swap(arr[i],arr[i+1]);
        }n--;

    }
   // for(int i=0;i<3;i++)
       // cout<<arr[i]<<" ";
       cout<<arr[2];
    return 0;
}
